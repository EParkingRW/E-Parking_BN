  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r esm src/index.js",
    "build": "rm -rf dist && babel src --out-dir dist --source-maps inline --copy-files",
    "start-test": "babel-node ./dist/index.js",
    "start": "npm run build && babel-node ./dist/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seeding": "npx sequelize db:seed:all",
    "seed:undo": "npx sequelize db:seed:undo:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrateDB": "npx sequelize-cli db:migrate:undo:all && npm run migrate ",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop"
  },
